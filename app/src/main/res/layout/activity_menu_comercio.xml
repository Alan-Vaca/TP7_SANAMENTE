<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="menuMis_Pedidos"
    tools:context=".MenuComercio">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/login_toolbar2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="MENU" />

    <Button
        android:id="@+id/button25"
        android:layout_width="231dp"
        android:layout_height="57dp"
        android:onClick="MiUsuarioComercio"
        android:text="MI COMERCIO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button35" />

    <Button
        android:id="@+id/button34"
        android:layout_width="231dp"
        android:layout_height="57dp"
        android:layout_marginTop="68dp"
        android:onClick="MenuPedidos_Comerciante"
        android:text="MIS PEDIDOS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button27" />

    <Button
        android:id="@+id/button26"
        android:layout_width="231dp"
        android:layout_height="57dp"
        android:layout_marginTop="80dp"
        android:onClick="MenuMis_Productos"
        android:text="MI CATALOGO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button34" />

    <Button
        android:id="@+id/button33"
        android:layout_width="231dp"
        android:layout_height="57dp"
        android:layout_marginTop="8dp"
        android:onClick="MenuAgregarProducto"
        android:text="NUEVO PRODUCTO"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button26" />

    <Button
        android:id="@+id/button27"
        android:layout_width="126dp"
        android:layout_height="41dp"
        android:onClick="MenuHistorial"
        android:text="HISTORIAL"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button25" />

    <Button
        android:id="@+id/button35"
        android:layout_width="209dp"
        android:layout_height="41dp"
        android:layout_marginTop="28dp"
        android:onClick="MenuGenerarReporte"
        android:text="GENERAR REPORTE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_toolbar2" />

    <Button
        android:id="@+id/button29"
        android:layout_width="231dp"
        android:layout_height="57dp"
        android:onClick="MenuPrincipal"
        android:text="SALIR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button33" />
</androidx.constraintlayout.widget.ConstraintLayout>