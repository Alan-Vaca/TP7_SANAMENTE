<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Metodo_De_Pago">

    <Button
        android:id="@+id/button18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancelar"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="609dp" />

    <EditText
        android:id="@+id/editTextText13"
        android:layout_width="120dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Fecha"
        android:inputType="text"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="425dp" />

    <EditText
        android:id="@+id/editTextText12"
        android:layout_width="151dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="text"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="425dp" />

    <EditText
        android:id="@+id/editTextText11"
        android:layout_width="347dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Numero de tarjeta"
        android:inputType="text"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="343dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Método de pago" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Complete los datos"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="108dp" />

    <EditText
        android:id="@+id/editTextText10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Código"
        android:inputType="text"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="497dp" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="251dp"
        android:layout_height="31dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="269dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo de tarjeta"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="230dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Finalizar compra"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="609dp" />
</androidx.constraintlayout.widget.ConstraintLayout>